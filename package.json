    "license": "MIT",
    "main": "dist/wrapper.js",
    "types": "dist/wrapper.d.ts",
    "gypfile": true,
    "scripts": {
        "install": "(prebuild-install || npm run build) && tsc",
        "test": "cross-env UV_THREADPOOL_SIZE=32 node --napi-modules ./test/test_binding.js",
