        "clean": "node-gyp clean",
        "prebuild": "prebuild -t 3 -r napi -a x64 --strip",
        "prebuild-upload": "prebuild --upload-all -u %GITHUB_TOKEN%"
    },
    "binary": {
        "napi_versions": [
            3
        ]
      },
    "devDependencies": {
        "@types/node": "^20.2.5",
        "cross-env": "^7.0.3",
        "prebuild": "^11.0.2",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "node-addon-api": "^6.1.0",
        "prebuild-install": "^7.1.1",
        "tiny-typed-emitter": "^2.1.0"
    }
}
